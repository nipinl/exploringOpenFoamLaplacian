FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 350;
    }
    right
    {
        type            zeroGradient;
    }
    top
    {
        type            fixedValue;
        value           uniform 300;
    }
    bottom
    {
        type            fixedValue;
        value           uniform 400;
    }
    frontAndBack
    {
        type            empty;
    }
}

